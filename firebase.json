{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "dist/ur_admin_site",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
    {
        "source": "/messages",
        "destination": "/browser/messages/index.html"
      },{
        "source": "/main",
        "destination": "/browser/main/index.html"
      },{
        "source": "/",
        "destination": "/browser/index.csr.html"
      },
      {
        "source": "**",
        "destination": "/browser/index.html"
      }
    
    ],
    "headers": [{
      "source": "**",
      "headers": [{
          "key": "Access-Control-Allow-Origin",
          "value": "*"
      }]
    }]
  },
  "emulators": {
    "hosting": {
      "port": 5000,
      "host": "0.0.0.0"
    },
    "auth": {
      "port": 9099
    },
    "database": {
      "port": 9000
    },
    "ui": {
      "enabled": true
    },
    "singleProjectMode": true
  }
}
